        "#2 dimensional vector and plot\n",
        "\n",
        "\n",
        "fig,ax = plt.subplots(1)\n",
        "\n",
        "xx = 4 # X Vector\n",
        "yy = 2 # Y Vector \n",
        "plt.arrow(0, 0, xx, yy, head_width=.1, head_length=0.1, color = 'pink')\n",
        "\n",
        "plt.xlim(-1,5) # Set plot X dimensions\n",
        "plt.ylim(-1,5) # Set plot Y dimensions\n",
        "\n",
